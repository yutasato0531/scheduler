    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "lint": "^0.8.19",
    "mocha": "^10.8.2",
    "pg": "^8.16.0",
    "postgres": "^3.4.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
